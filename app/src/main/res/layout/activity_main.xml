<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:background="#dedede"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="fssg.filesafesg.MainActivity">



    <Button
        android:text="Button"
        android:id="@+id/button9"
        android:layout_height="70dp"
        android:layout_width="250dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Photos"
        android:id="@+id/photos"
        android:layout_height="70dp"
        android:layout_width="250dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/button9"
        android:layout_alignStart="@+id/button9"
        android:background="@android:color/holo_blue_bright" />




    <Button
        android:text="Videos"
        android:id="@+id/videos"
        android:layout_height="70dp"
        android:layout_width="250dp"
        android:background="@android:color/holo_green_light"
        android:layout_below="@+id/button9"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Audio"
        android:id="@+id/audio"
        android:layout_height="70dp"
        android:layout_width="250dp"
        android:background="@android:color/holo_orange_dark"
        android:layout_below="@+id/videos"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Documents"
        android:id="@+id/documents"
        android:layout_height="70dp"
        android:layout_width="250dp"
        android:background="@android:color/holo_red_light"
        android:layout_below="@+id/audio"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Encrypted Files"
        android:id="@+id/encryptedfiles"
        android:layout_height="70dp"
        android:layout_width="250dp"
        android:background="@android:color/background_light"
        android:layout_below="@+id/documents"
        android:layout_alignLeft="@+id/documents"
        android:layout_alignStart="@+id/documents" />
</RelativeLayout>
